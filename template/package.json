{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:pods": "npx pod-install",
    "start": "react-native start",
    "web": "next dev",
    "build": "next build",
    "next-start": "next start",
    "build-web": "react-app-rewired build",
    "test:all": "yarn test:jest",
    "test:jest": "jest",
    "lint:all": "yarn lint:eslint && yarn lint:prettier && yarn lint:types",
    "lint:eslint": "eslint src/",
    "lint:eslint:fix": "eslint --fix src/",
    "lint:prettier": "prettier --check \"src/**/*.+(js|jsx|json|css|md)\"",
    "lint:prettier:fix": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\"",
    "lint:types": "tsc",
    "package:install": "yarn && yarn ios:pods || true",
    "package:update": "yarn upgrade --latest && cd ios && rm -f Podfile.lock && yarn ios:pods && pod update",
    "postinstall": "patch-package",
    "next-lint": "next lint"
  },
  "dependencies": {
    "@expo/config": "^7.0.1",
    "@expo/next-adapter": "^4.0.12",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/material-top-tabs": "^6.2.4",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.1",
    "@tsconfig/react-native": "^2.0.2",
    "babel-plugin-react-native-web": "^0.18.9",
    "next": "^13.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-native": "^0.70.3",
    "react-native-gesture-handler": "2.7.0",
    "react-native-pager-view": "^6.0.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.0",
    "react-native-tab-view": "^3.2.1",
    "react-native-web": "^0.18.9",
    "solito": "^1.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/runtime": "^7.19.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@types/jest": "26.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-native": "^0.70.4",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.8",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "^0.73.0",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-images": "^1.8.4",
    "next-transpile-modules": "^9.1.0",
    "patch-package": "^6.4.7",
    "pod-install": "^0.1.38",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.6.0",
    "react-test-renderer": "^18.1.0",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1"
  },
  "resolutions": {
    "@babel/plugin-transform-typescript": "7.19.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|react-native-vector-icons|solito)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
