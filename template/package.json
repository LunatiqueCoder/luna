{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:pods": "npx pod-install",
    "start": "react-native start",
    "web": "next dev",
    "build": "next build",
    "next-start": "next start",
    "test:all": "yarn test:jest",
    "test:jest": "jest",
    "lint:all": "yarn lint:eslint && yarn lint:prettier && yarn lint:types",
    "lint:eslint": "eslint src/",
    "lint:eslint:fix": "eslint --fix src/",
    "lint:prettier": "prettier --check \"src/**/*.+(js|jsx|json|css|md)\"",
    "lint:prettier:fix": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\"",
    "lint:types": "tsc",
    "package:install": "yarn && yarn ios:pods || true",
    "package:update": "yarn upgrade --latest && cd ios && rm -f Podfile.lock && yarn ios:pods && pod update",
    "next-lint": "next lint",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/drawer": "^6.5.6",
    "@react-navigation/native": "^6.1.1",
    "@tamagui/animations-react-native": "^1.0.3",
    "@tamagui/babel-plugin": "^1.0.3",
    "@tamagui/font-inter": "^1.0.3",
    "@tamagui/font-silkscreen": "^1.0.3",
    "@tamagui/lucide-icons": "^1.0.3",
    "@tamagui/next-plugin": "^1.0.3",
    "@tamagui/next-theme": "^1.0.3",
    "@tamagui/shorthands": "^1.0.3",
    "@tamagui/theme-base": "^1.0.3",
    "expo": "^47.0.0",
    "expo-linear-gradient": "~12.0.1",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.70.6",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-pager-view": "^6.1.2",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.6.0",
    "react-native-tab-view": "^3.3.4",
    "react-native-web": "^0.18.10",
    "sharp": "^0.31.3",
    "solito": "^2.1.2",
    "tamagui": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.20.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.2.4",
    "@types/react-dom": "^18.0.9",
    "@types/react-native": "^0.70.8",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^7.32.0",
    "eslint-config-next": "^13.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.73.6",
    "patch-package": "^6.5.1",
    "pod-install": "^0.1.38",
    "postinstall": "^0.8.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "resolutions": {
    "@babel/plugin-transform-typescript": "7.19.3",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|react-native-vector-icons|solito|@react-navigation)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
